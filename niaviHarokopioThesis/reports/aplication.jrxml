	<queryString>
		<![CDATA[SELECT a.id,
     to_char(birth_date,'dd/MM/yyyy') AS birth_date,
     name,
     surname,
     afm,
     b."motherName" AS b_motherName,
     b."fatherName" AS b_fatherName,
     c.first_choice,
     c.second_choice,
     c.third_choice,
     c.studies_type,
     c.other_applications,
     c.reason_of_selection,
     p.profile_pic
FROM
 "personal_basicuserinfo" b
left outer join personal_application a on b.application_id =a.id
left outer join personal_additionalinfo c on c.application_id =a.id
left outer join personal_profile p on p.user_id = a.user_id
WHERE
a.id = $P{APPLICATION_ID}
 and   b.user_id =  a.user_id]]>
	</queryString>
